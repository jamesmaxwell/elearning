@model IEnumerable<System.Security.Claims.Claim>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>授权列表</h2>
<ul>
    @foreach (var claim in Model)
    {
        <li>@claim.Type,@claim.Value</li>
        <ul>
            @foreach (var priv in claim.Properties)
            {
                <li>@priv.Key,@priv.Value</li>
            }
        </ul>
    }
</ul>